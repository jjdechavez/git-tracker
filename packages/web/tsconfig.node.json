{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@console/functions/*": ["../../functions/src/*"],
      "@console/core/*": ["../../core/src/*"],
      "$/*": ["./src/*"]
    }
  },
  "include": ["vite.config.ts"]
}
